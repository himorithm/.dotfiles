# Himanshu Ahire

# Import Keybord bindings
include ~/.config/sway/keybindings

# Terminal emulator
set $term alacritty

# Application launcher
set $menu wofi -iIS run 

# Wallpapers
#exec ~/.config/sway/scripts/change_random_wallpaper.sh
output "*" bg  ~/.wallpaper/wallpaper.jpg fill

# Status Bars
include ~/.config/sway/waybar

# Themes
include ~/.config/sway/themes/gruvbox
#include ~/.config/sway/themes/nord

# Application Management
include ~/.config/sway/startupapps

# Defaults
include /etc/sway/config.d/*
